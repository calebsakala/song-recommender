    "def sq_dist(a, b):\n",
    "#     Args: a (ndarray) (n,): vector with n features\n",
    "#     Args: b (ndarray) (n,): vector with n features\n",
    "    n = a.shape[0]\n",
    "    d = 0\n",
    "    for i in range(n):\n",
    "        d+= (a[i] - b[i])**2\n",
    "    return d"
   ]
